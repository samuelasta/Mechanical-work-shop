### OrdenesController Endpoints

# Crear orden para un vehículo
POST http://localhost:8080/api/ordenes/{idVehiculo}/vehiculo
Content-Type: application/json

{
  "fechaIngreso": "2023-10-01T10:00:00",
  "descripcion": "Revisión general del vehículo"
}

###

# Eliminar orden
DELETE http://localhost:8080/api/ordenes/{idOrden}

###

# Actualizar orden
PUT http://localhost:8080/api/ordenes/{idOrden}
Content-Type: application/json

{
  "descripcion": "Cambio de aceite y filtros",
  "fechaIngreso": "2023-10-01T10:00:00",
  "fechaSalida": "2023-10-02T15:00:00"
}

###

# Obtener orden
GET http://localhost:8080/api/ordenes/{idOrden}

###

# Listar todas las órdenes
GET http://localhost:8080/api/ordenes

###

# Asignar mecánico a orden
PATCH http://localhost:8080/api/ordenes/{idOrden}/mecanicos/{idMecanico}
Content-Type: application/json

{
  "rol": "MECANICO"
}

###

# Eliminar mecánico de orden
DELETE http://localhost:8080/api/ordenes/{idOrden}/mecanicos/{idMecanico}

###

# Obtener mecánicos por orden
GET http://localhost:8080/api/ordenes/{idOrden}/mecanicos

###

# Registrar diagnóstico
POST http://localhost:8080/api/ordenes/{idOrden}/diagnosticos
Content-Type: application/json

{
  "diagnosticoInicial": "Problema en el motor",
  "diagnosticoFinal": "Cambio de bujías requerido"
}

###

# Obtener diagnóstico
GET http://localhost:8080/api/ordenes/{idOrden}/diagnosticos

###

# Registrar servicio en orden
POST http://localhost:8080/api/ordenes/{idOrden}/mecanicos/{idMecanico}/servicios/{idServicio}
Content-Type: application/json

{
  "rol": "MECANICO",
  "horasTrabajadas": 2,
  "fechaAsignacion": "2023-10-01T10:00:00"
}

###

# Actualizar detalle de servicio
PUT http://localhost:8080/api/ordenes/{idOrden}/mecanicos/{idMecanico}/servicios/{idServicio}
Content-Type: application/json

{
  "rol": "SUPERVISOR",
  "horasTrabajadas": 3,
  "fechaAsignacion": "2023-10-01T10:00:00"
}

###

# Obtener detalle de orden
GET http://localhost:8080/api/ordenes/{idOrden}/detalle

